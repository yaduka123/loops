
<html>
<head>
<script>
function factors(){
var n = parseInt(document.getElementById("num").value);
var count=0;
for(var i=1 ; i<=n ; i++){
if(n%i==0)
count++;
}
document.write("Factors count is : " + count);
}
</script>
</head>
<body>
Enter Number : <input type="text" id="num"/>
<button onclick="factors()"> Count Factors </button>
</body>
</html>